{
    "manifest_version": 2,
    "name": "Material You NewTab",
    "version": "2.1",
    "description": "A Simple New Tab (browser's home page) inspired by Google's 'Material You' design.",
    "permissions": [],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "icons": {
        "48": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}